{
  "trailingComma": "all",
  "tabWidth": 2,
  "vueIndentScriptAndStyle": true,
  "semi": true,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "printWidth": 100,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "quoteProps": "consistent",
  "useTabs": false,
  "endOfLine": "auto"
}
